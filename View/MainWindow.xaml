<Window x:Class="PasswordSaver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordSaver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        ResizeMode="NoResize"
        WindowStyle="None">
    <Grid>
        <Grid Margin="0,-420,0,0" Height="30" Background="DarkGray">
            <Button x:Name="btnMinimize" Content="_" Padding="0,-15,0,0" FontSize="22" Margin="732,5,48,5" Height="20" Width="20" Background="DimGray" Click="btnMinimize_Click"/>
            <Button x:Name="btnClose" Content="X" Padding="0,-5,0,0" FontSize="20" Margin="768,5,12,5" Height="20" Width="20" Background="DimGray" Click="btnClose_Click"/>
            <Label Name="WindowName" FontSize="14" FontWeight="DemiBold" Margin="0,0,0,0" Height="30" Width="150" Padding="15,5,0,0" Content="Password Saver 1.0"/>
        </Grid>
        <Frame Height="420" Margin="0,30,0,0" Source="pageFrontPage.xaml" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
